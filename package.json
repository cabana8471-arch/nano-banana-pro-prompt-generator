    "nanoid": "^5.1.6",
    "next": "16.0.7",
    "next-intl": "^4.5.8",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/file-saver": "^2.0.7",
